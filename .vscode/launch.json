{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flutter (dev)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "cwd": "${workspaceFolder}",
            "args": ["--flavor", "dev", "--target", "lib/main_dev.dart"]
        },
        {
            "name": "Flutter (prod)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "cwd": "${workspaceFolder}",
            "args": ["--flavor", "prod", "--target", "lib/main.dart"]
        },
        {
            "name": "Flutter (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "Flutter (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}
